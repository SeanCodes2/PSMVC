@{
    ViewData["Title"] = "Home Page";
}
@*Header*@
<header class="bg-dark py-5">
    <div class="container px-5">
        <div class="row gx-5 align-items-center justify-content-center">
            <div class="col-lg-8 col-xl-7 col-xxl-6">
                <div class="my-5 text-center text-xl-start">
                    <h1 class="display-5 fw-bolder text-white mb-2 ">Welcome to my website!</h1>
                    <h3 class=" fw-bolder text-white mb-2 ">Sean Coyan</h3>
                    <p class="lead fw-normal text-white-50 mb-4">Full Stack Software Developer </p>
                    <div class="d-grid gap-3 d-sm-flex justify-content-sm-center justify-content-xl-start">
                        <a class="btn btn-primary btn-sm px-4 me-sm-3" style="font-size: 2em;" href="@Url.Action("Resume", "Home")">Portfolio</a>
                        @*<a class="btn btn-primary btn-lg px-4 me-sm-3" href="@Url.Action("Contact", "Home")">Contact Me!</a>  *@
                        @*<i class="bi bi-linkedin" style="font-size: 2em"></i>*@
                        <a class="btn btn-primary btn-sm px-4 me-sm-3" href="https://www.linkedin.com/in/seancoyan/" target="_blank"><i class="bi bi-linkedin" style="font-size: 2em"></i></a>
                        <a class="btn btn-primary btn-sm px-4" href="https://github.com/SeanCodes2" target="_blank"><i class="bi bi-github" style="font-size: 2em"></i></a>
                    </div>
                </div>
            </div>
            <div class="col-xl-5 col-xxl-6 d-none d-xl-block text-center"><img class="img-fluid rounded-4 my-5" src="/assets/images/seancoyan1_websize.jpg" alt="Picture of Sean Coyan" /></div>
        </div>
    </div>
</header>
